
#fibonacci series

def fib(n):
    
    a = 0
    b = 1
    
    while n >= 1:
        
        if n == 1:
            print(a)
            break
      
        else:
            print(a)
            print(b)
            for i in range(2,n):
                c = a + b
                a = b
                b = c
                
                print(c)
            break
    else :
        
        print("Please Enter positive Number")
        
fib(6)
